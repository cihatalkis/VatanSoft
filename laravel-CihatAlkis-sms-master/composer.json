  "keywords": [
    "laravel",
    "sms",
    "api",
    "VatanSoft"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Cihat Alkis",
      "email": "cihjatalkis@icloud.com",
      "homepage": "https://cihatalkis.pw",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "9.*",
    "guzzlehttp/guzzle": "^6.3.1|^7.0",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {
      "CihatAlkis\\VatanSoft\\": "src/CihatAlkis/VatanSoft/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [ "CihatAlkis\\VatanSoft\\VatanSoftServiceProvider"
